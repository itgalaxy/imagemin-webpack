{
  "name": "imagemin-webpack",
  "version": "4.0.1",
  "description": "Webpack loader and plugin to optimize (compress) images using imagemin",
  "repository": {
    "type": "git",
    "url": "https://github.com/itgalaxy/imagemin-webpack.git"
  },
  "homepage": "https://github.com/itgalaxy/imagemin-webpack",
  "keywords": [
    "webpack",
    "loader",
    "plugin",
    "imagemin",
    "images",
    "minify",
    "compress",
    "optimize"
  ],
  "author": "Itgalaxy <development@itgalaxy.company>",
  "bugs": {
    "url": "https://github.com/itgalaxy/imagemin-webpack/issues"
  },
  "license": "MIT",
  "main": "index.js",
  "files": [
    "src"
  ],
  "engines": {
    "node": ">= 6.9.0 || >= 8.9.0"
  },
  "dependencies": {
    "async-throttle": "^1.1.0",
    "cacache": "^11.0.1",
    "find-cache-dir": "^2.0.0",
    "serialize-javascript": "^1.6.0",
    "loader-utils": "^1.0.0",
    "imagemin": "^6.0.0",
    "nodeify": "^1.0.0",
    "webpack-sources": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "del": "^3.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-ava": "^5.1.1",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-itgalaxy": "^95.0.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-lodash": "^5.1.0",
    "eslint-plugin-markdown": "^1.0.0-rc.1",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-unicorn": "^6.0.1",
    "file-loader": "^3.0.1",
    "url-loader": "^1.0.1",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^6.0.0",
    "imagemin-svgo": "^7.0.0",
    "lint-staged": "^8.0.4",
    "lodash.defaultsdeep": "^4.6.0",
    "npm-run-all": "^4.0.0",
    "npmpub": "^4.0.1",
    "pify": "^4.0.1",
    "prettier": "^1.5.2",
    "remark-cli": "^6.0.0",
    "remark-preset-lint-itgalaxy": "^13.0.0",
    "tempy": "^0.2.0",
    "webpack": "^4.0.1"
  },
  "peerDependencies": {
    "webpack": "^4.0.0"
  },
  "scripts": {
    "lint:prettify": "prettier --list-different '{**/*,*}.{js,mjs,jsx,json,md}' --ignore-path .gitignore",
    "lint:js": "eslint . --cache --report-unused-disable-directives --ignore-path .gitignore --ext 'js,.mjs,.jsx,.md'",
    "lint:md": "remark . LICENSE -f -q -i .gitignore",
    "lint": "npm-run-all -l -p 'lint:**'",
    "prettify": "npm run lint:prettify -- --write",
    "fix:js": "npm run lint:script -- --fix",
    "fix": "npm-run-all -l prettify -p 'fix:**'",
    "pretest": "npm run lint",
    "test": "npm run test-only",
    "test-only": "jest --runInBand --coverage",
    "release": "npmpub"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "6.9.0"
          },
          "useBuiltIns": "usage"
        }
      ]
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "script"
    },
    "rules": {
      "node/no-unpublished-require": [
        "error",
        {
          "allowModules": [
            "imagemin-gifsicle",
            "imagemin-svgo"
          ]
        }
      ],
      "import/no-unresolved": [
        "error",
        {
          "ignore": [
            "imagemin-webpack"
          ]
        }
      ]
    },
    "extends": [
      "plugin:itgalaxy/node",
      "plugin:itgalaxy/esnext",
      "plugin:itgalaxy/jest",
      "plugin:itgalaxy/markdown"
    ],
    "overrides": [
      {
        "files": [
          "**/__tests__/**/*.js"
        ],
        "parserOptions": {
          "sourceType": "module"
        },
        "rules": {
          "node/no-unsupported-features/es-syntax": "off"
        }
      },
      {
        "files": [
          "**/*.md"
        ],
        "parserOptions": {
          "ecmaFeatures": {
            "impliedStrict": true
          }
        },
        "rules": {
          "strict": "off",
          "no-undef": "off",
          "no-unused-vars": "off",
          "no-process-env": "off",
          "no-process-exit": "off",
          "no-console": "off",
          "import/no-unresolved": "off",
          "node/no-unpublished-require": "off"
        }
      }
    ],
    "root": true
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-itgalaxy"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/",
      "helpers.js"
    ]
  }
}
